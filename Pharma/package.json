{
  "name": "pharma-vcf-backend",
  "version": "1.0.0",
  "description": "VCF file upload and parsing backend",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-files",
    "copy-files": "node -e \"require('fs').cpSync('src/config', 'dist/config', {recursive: true})\"",
    "start": "node dist/index.js",
    "dev": "npx ts-node src/index.ts",
    "watch": "tsc --watch"
  },
  "keywords": [
    "vcf",
    "genomics",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "cors": "^2.8.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.6.3",
    "multer": "^1.4.5-lts.1",
    "openai": "^6.22.0",
    "vcf": "^2.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/multer": "^1.4.10",
    "@types/node": "^20.8.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
